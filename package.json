{
  "name": "eval-next",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "env:dev": "vercel env pull .env",
    "env:prod": "vercel env pull --environment=production .env",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "npm run db:reset:confirm",
    "db:reset:confirm": "prisma migrate reset --force && prisma generate",
    "db:reset:dev": "prisma db push --force-reset && prisma generate",
    "db:reset:script": "./scripts/db-reset.sh",
    "db:reset:production": "./scripts/db-reset-prod.sh",
    "db:seed": "tsx prisma/seed.ts",
    "clerk:reset": "./scripts/clerk-users-reset.sh",
    "clerk:reset:force": "./scripts/clerk-users-reset.sh --force",
    "db:seed:profiles": "tsx prisma/seed-game-profiles.ts",
    "db:reset:full": "npm run db:reset:confirm && npm run db:seed",
    "dev": "concurrently \"next dev --turbo\" \"ngrok http --url=fluent-peacock-grown.ngrok-free.app 3000\"",
    "dev:next": "next dev --turbo",
    "dev:ngrok": "ngrok http --url=bison-pleased-unicorn.ngrok-free.app 3000",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate && prisma migrate deploy && tsx prisma/seed_production.ts",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "concurrently \"next start\" \"ngrok http --url=bison-pleased-unicorn.ngrok-free.app 3000\"",
    "typecheck": "tsc --noEmit"
  },
